{
  "name": "mithril-redux",
  "version": "1.0.0",
  "description": "A Mithril boilerplate for fast development",
  "main": "src/main.js",
  "repository": "https://github.com/abradley2/mithril-redux",
  "author": "Tony Bradley <antbradley91@gmail.com>",
  "license": "Unlicense",
  "scripts": {
    "start": "budo ./src/main.js:main.bundle.js --pushstate --live --dir ./public",
    "build": "browserify ./src/main.js -g uglifyify -o ./public/main.bundle.js"
  },
  "browserify": {
    "transform": [
      "sheetify/transform",
      "hyperxify",
      "es2020"
    ]
  },
  "dependencies": {
    "hyperx": "^2.0.5",
    "icepick": "^1.3.0",
    "mithril": "^1.0.1",
    "redux": "^3.6.0",
    "reselect": "^2.5.4"
  },
  "devDependencies": {
    "browserify": "^14.1.0",
    "budo": "^9.4.7",
    "envify": "^4.0.0",
    "es2020": "^1.1.9",
    "hyperxify": "^2.0.1",
    "sheetify": "^6.0.1",
    "standard": "^9.0.0",
    "uglifyify": "^3.0.4",
    "watchify": "^3.9.0"
  }
}
